{
  "name": "astronomia",
  "version": "1.4.0",
  "description": "An astronomical library",
  "keywords": [
    "astronomy",
    "julian",
    "meeus",
    "planetposition"
  ],
  "homepage": "https://github.com/commenthol/astronomia",
  "bugs": {
    "url": "https://github.com/commenthol/astronomia"
  },
  "license": "MIT",
  "author": "commenthol <commenthol@gmail.com>",
  "main": "lib",
  "module": "src",
  "repository": {
    "type": "git",
    "url": "https://github.com/commenthol/astronomia.git"
  },
  "scripts": {
    "all": "npm run clean && npm run lint && npm run build && npm run test",
    "build": "babel -d lib src",
    "clean": "rimraf lib doc coverage .nyc_output",
    "coverage": "nyc -r text -r html npm test",
    "lint": "eslint '**/*.js'",
    "prepublishOnly": "npm run all",
    "test": "mocha",
    "zuul": "zuul test/*.js"
  },
  "babel": {
    "presets": [
      "env",
      "stage-2"
    ]
  },
  "eslintConfig": {
    "extends": "standard",
    "plugins": [
      "standard"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "babelify": "^8.0.0",
    "core-js": "^2.5.3",
    "eslint": "^4",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3",
    "eslint-plugin-standard": "^3",
    "mocha": "^5.0.1",
    "nyc": "^11.4.1",
    "rimraf": "^2.6.2",
    "serialize-to-js": "^1.2.0"
  },
  "engines": {
    "node": ">=0.12"
  },
  "maintainers": [
    "commenthol <commenthol@gmail.com>"
  ]
}
