{
  "name": "astronomia",
  "version": "1.3.2",
  "description": "An astronomical library",
  "main": "lib",
  "jsnext:main": "src",
  "scripts": {
    "dist": "npm run clean && npm run lint && npm run transpile && npm run test",
    "test": "mocha -R dot",
    "cover": "nyc npm test && nyc report --reporter=html",
    "lint": "eslint '**/*.js'",
    "transpile": "babel -d lib src",
    "clean": "rimraf doc coverage .nyc_output"
  },
  "author": "commenthol <commenthol@gmail.com>",
  "maintainers": "commenthol <commenthol@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=0.12"
  },
  "dependencies": {
    "lodash.padstart": "^4.6.1"
  },
  "devDependencies": {
    "babel-core": "^6",
    "babel-polyfill": "^6",
    "babel-preset-es2015": "^6",
    "babel-preset-stage-0": "^6",
    "babel-preset-stage-1": "^6",
    "babel-preset-stage-2": "^6",
    "mocha": "^3"
  },
  "optionalDependencies": {
    "babel-cli": "^6",
    "eslint": "^3",
    "eslint-config-standard": "^6",
    "eslint-plugin-promise": "^3",
    "eslint-plugin-standard": "^2",
    "nyc": "^10.1.2",
    "rimraf": "^2.5.4",
    "serialize-to-js": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/commenthol/astronomia.git"
  },
  "bugs": {
    "url": "https://github.com/commenthol/astronomia"
  },
  "homepage": "https://github.com/commenthol/astronomia",
  "keywords": [
    "astronomy",
    "meeus",
    "julian",
    "planetposition"
  ],
  "eslintConfig": {
    "extends": "standard",
    "plugins": [
      "standard"
    ],
    "rules": {}
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "stage-1",
      "stage-2"
    ]
  }
}
